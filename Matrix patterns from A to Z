      print("*",end=" ")
    else:
      print(" ",end=" ")
  print()

#ALPHABET "G" 
n=7
for rows in range(n):
  for columns in range(n):
    if columns==0 or rows==n-1  or rows==0 or columns==n-1 and rows>=n//2 or rows==n//2 and columns>=n//2:
      print("*",end=" ")
    else:
      print(" ",end=" ")
  print()

