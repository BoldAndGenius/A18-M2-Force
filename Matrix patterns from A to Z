#ALPHABET "A"
n=7
for rows in range(n):
  for columns in range(n):
    if columns==0 or rows==n//2 or columns==n-1 or rows==0:
      print("*",end=" ")
    else:
      print(" ",end=" ")
  print()



# ALPHABET "B"
n=7
for rows in range(n):
  for columns in range(n):
    if columns==n-1 and rows==n//2 or columns==n-1 and rows==0 or columns==n-1 and rows==n-1:
      print(" ",end=" ")
    elif columns==0 or rows==n//2  or columns==n-1  or rows==0  or rows==n-1  :
      print("*",end=" ")
  
   
    else:
        print(" ",end=" ")
  print()



#ALPHABET "C" 
n=7
for rows in range(n):
  for columns in range(n):
    if columns==0 or rows==n-1 or rows==0:
      print("*",end=" ")
    else:
      print(" ",end=" ")
  print()

#ALPHABET "D" 
n=7
for rows in range(n):
  for columns in range(n):
    if  columns==n-1 and rows==0 or columns==n-1 and rows==n-1:
      print(" ",end=" ")

    elif columns==0 or rows==n-1 or rows==0 or columns==n-1:
      print("*",end=" ")
    else:
      print(" ",end=" ")
  print()

#ALPHABET "E" 
